{
  "name": "a3al",
  "productName": "A3AL",
  "version": "1.0.0-RC1",
  "description": "ARMA3 alternative launcher",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint app test *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MustD/a3al.git"
  },
  "author": {
    "name": "mustd"
  },
  "license": "MIT",
  "keywords": [
    "arma3",
    "launcher",
    "linux"
  ],
  "devDependencies": {
    "asar": "0.13.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "del": "2.2.2",
    "devtron": "1.4.0",
    "electron": "1.4.15",
    "electron-devtools-installer": "2.1.0",
    "electron-packager": "8.5.1",
    "electron-rebuild": "1.5.7",
    "eslint": "3.14.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fbjs-scripts": "0.7.1",
    "image-webpack-loader": "3.2.0",
    "jsdom": "9.9.1",
    "json-loader": "0.5.4",
    "minimist": "1.2.0",
    "mocha": "3.2.0",
    "node-libs-browser": "2.0.0",
    "react-addons-test-utils": "15.4.2",
    "redux-logger": "2.7.4",
    "sinon": "1.17.7",
    "spectron": "3.5.0",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-merge": "2.6.0"
  },
  "dependencies": {
    "css-modules-require-hook": "4.0.5",
    "electron-debug": "1.1.0",
    "electron-json-storage": "3.0.0",
    "immutable": "3.8.1",
    "material-ui": "0.16.7",
    "postcss": "5.2.11",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "source-map-support": "0.4.11"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x || 7.x",
    "npm": "2.x || 3.x || 4.x"
  }
}
